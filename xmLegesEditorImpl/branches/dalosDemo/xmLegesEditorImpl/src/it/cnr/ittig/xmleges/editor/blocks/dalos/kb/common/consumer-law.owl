<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:iol="http://www.loa-cnr.it/ontologies/IOLite.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:nor="http://www.loa-cnr.it/ontologies/CLO/NormativePositions.owl#"
    xmlns:dul="http://www.loa-cnr.it/ontologies/DUL.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://turing.ittig.cnr.it/jwn/ontologies/consumer-law.owl#"
    xmlns:clo="http://www.loa-cnr.it/ontologies/CLO/CoreLegal.owl#"
    xmlns:p4="http://www.loa-cnr.it/ontologies/DULowl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://turing.ittig.cnr.it/jwn/ontologies/consumer-law.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="immovable_property">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Physical_Object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="legal-person">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isActedBy"/>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="physical_agent"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="agent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="social_object">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >all objects that have been or are created in the process of social communication, which are assumed to have a real existence in the social world</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="access_to_consumer_goods">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="activities"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="period-of-the-year">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="contract-duration"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="contracts_relating_to_the_purchase_of_the_right_to_use_immovable_properties_on_a_timeshare_basis"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is-duration-of"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="creditor">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="consumer"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="natural_person"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is-consumer-role-of"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="credit-agreement"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isRoleDefinedIn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="single_liaison_office">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >means the public authority in each Member State designated as responsible for coordinating the application of this Regulation within that Member State. reg.2006/2004</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="consumer_law-authority"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="service_provider">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="service_providing"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is_participant_in"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is-seller-role-of"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#agent"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="legal_role"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="consumer-complaint">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >consumer complaint" means a statement, supported by reasonable evidence, that a seller or supplier has committed, or is likely to commit, an infringement of the laws that protect consumers' interests</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="infringment"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="has_object"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="consumer-protection"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="representative">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#legal_role"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="mutual_assistance">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#activities"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="organization">
    <rdfs:subClassOf rdf:resource="#legal-person"/>
  </owl:Class>
  <owl:Class rdf:ID="advertising">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isAbout"/>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="consumer_goods"/>
              <owl:Class rdf:ID="services"/>
              <owl:Class rdf:about="#immovable_property"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="commercial_communication"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the making of a representation in any form in connection with a trade, business, craft or profession in order to promote the supply of goods or services, including immovable property, rights and obligations(1984/450)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#natural_person">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#physical_agent"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#agent"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="geographical_address"/>
              <owl:Class rdf:ID="legal_address"/>
              <owl:Class rdf:ID="residence"/>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasQuality"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="raw_material">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="product"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#product"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#legal_address">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Quality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is_quality_of"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#agent"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="defective">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="quality_region"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is_quality_value"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#consumer_goods"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="operator-of-a_means-of-communication">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isParticipantIn"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#commercial_communication"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="supplier"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >means any public or private natural or legal person whose trade, business or profession involves making one or more means of distance communication available to suppliers 1993-13</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="time-interval">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="temporal-region"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="legal_document">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="legal_object"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="movable-object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="damage">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="legal_situation"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >damage 
(a) damage caused by death or by personal injuries; (b) damage to, or destruction of, any item of property other than the defective product itself, with a lower threshold of 500 ECU, provided that the item of property: (i) is of a type ordinarily intended for private use or consumption, and (ii) was used by the injured person mainly for his own private use or consumption (1985/374)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="legal_description">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A description of the ' ideal' view of law  concerning a human behaviour or a . a legal description  create a legal status or  a legal figure (by constitutive norms); classifies situations as legal  facts, qualifies the  normative position of an agent playing  a role</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#legal_object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="disclosure">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="market_survellance-activities"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="competent-authority">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#legal_role"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >means any public authority established either at national, regional or local level with specific responsibilities to enforce the laws that protect consumers' interests Reg.2006/2004</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="away_from_business_premises">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="spatial-region"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="sale-contract">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contratto: l'accordo che lega il consumatore all'organizzatore e/o al venditore. 1990/314</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasParticipant"/>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:ID="buyer"/>
                  <owl:Class rdf:about="#consumer"/>
                </owl:unionOf>
              </owl:Class>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:ID="organizer"/>
                  <owl:Class rdf:ID="producer"/>
                  <owl:Class rdf:ID="seller"/>
                  <owl:Class rdf:about="#supplier"/>
                  <owl:Class rdf:ID="trader"/>
                </owl:unionOf>
              </owl:Class>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#consumer"/>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:about="#organizer"/>
                  <owl:Class rdf:about="#seller"/>
                </owl:unionOf>
              </owl:Class>
            </owl:intersectionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#definesRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#product"/>
              <owl:Class rdf:about="#service_providing"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#has_object"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="contract"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="right_to_use_immovable_properties"/>
              <owl:Class rdf:ID="withdrawal_right"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="constitutes"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="e_commerce_contract">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="distance-contract"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Region">
    <owl:disjointWith>
      <owl:Class rdf:about="#activities"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#consumer_law-authority">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="institutional_body"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="leasing_contract">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#contract"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="inspection">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#market_survellance-activities"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="unit-price">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="selling-price"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >unit price 
the final price, including VAT and all other taxes, for one kilogramme, one litre, one metre, one square metre or one cubic metre of the product or a different single unit of quantity which is widely and customarily used in the Member State concerned in the marketing of specific products 1998/6</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#consumer-protection">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="normative-position"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a set of normative positions created by law to protect consumers interests</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Physical_Object">
    <rdfs:subClassOf rdf:resource="#object"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#is_participant_in"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#activities"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#selling-price">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1998/6
selling price 
the final price for a unit of the product, or a given quantity of the product, including VAT and all other taxes</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="cost"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#quality_region">
    <rdfs:subClassOf rdf:resource="#Region"/>
  </owl:Class>
  <owl:Class rdf:ID="offer">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="commercial_activity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="legal_text">
    <rdfs:subClassOf rdf:resource="#legal_document"/>
  </owl:Class>
  <owl:Class rdf:about="#market_survellance-activities">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="collective_interests_of_consumers"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasTask"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#collective_interests_of_consumers"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/CLO/NormativePositions.owl#hasPowerTowards"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#activities"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasParticipant"/>
        <owl:allValuesFrom rdf:resource="#consumer_law-authority"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >means the actions of a competent authority designed to detect whether intra-Community infringements have taken place within its territory;(2006/2004)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="finished_product">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#product"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="feauture_description">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="information_object"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#commercial_communication"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="is_object_of"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#spatial-region">
    <rdfs:subClassOf rdf:resource="#Region"/>
  </owl:Class>
  <owl:Class rdf:about="#agent">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A natural or legal person  which plays the role of  legal subject .Legal persons (companies, public bodies, organizations) are acted by  natural persons as  representatives</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="has-role"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#legal_role"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#access_to_consumer_goods"/>
              <owl:Class rdf:about="#commercial_activity"/>
              <owl:Class rdf:about="#market_survellance-activities"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#is_participant_in"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="three-years-period">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#contract-duration"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="supplier_of_a_means_of_distance_communication">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#supplier"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="misleading_advertising">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#legal_situation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#advertising"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/CLO/NormativePositions.owl#isLiabilityTargetOf"/>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#seller"/>
              <owl:Class rdf:about="#producer"/>
              <owl:Class rdf:about="#supplier"/>
              <owl:Class rdf:ID="professional"/>
              <owl:Class rdf:about="#trader"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >any advertising which in any way, including its presentation, deceives or is likely to deceive the persons to whom it is addressed or whom it reaches and which, by reason of its deceptive nature, is likely to affect their economic behaviour or which, for those reasons, injures or is likely to injure a competitor 450/1984</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="consumer_representative">
    <rdfs:subClassOf rdf:resource="#representative"/>
  </owl:Class>
  <owl:Class rdf:ID="unfair-terms">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#contract"/>
    </rdfs:subClassOf>
